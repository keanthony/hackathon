<%= form_for(@participant) do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :major %><br>
    <%= f.text_field :major %>
  </div>
  <div class="field">
    <%= f.label :concentration %><br>
    <%= f.text_field :concentration %>
  </div>
  <div class="field">
    9000 <%= f.label :id_number %><br>
    <%= f.text_field :id_number %>
  </div>
  <%= hidden_field_tag(:subaction, @event.id) %>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
